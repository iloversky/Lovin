@charset "utf-8";
/*公用*/
Body, div, p, ul, ol, dl, dt, dd, li, form, input,  img, h1, h2, h3, h4, h5, h6{margin:0;padding:0px;text-align:left; }
Body{color:#666;font-size:14px; font:12px/1.5 "微软雅黑","宋体"，Arial,Lucida,Verdana,Helvetica,sans-serif; position:relative;
scrollbar-arrow-color: #FFF; /*图6,三角箭头的颜色*/
scrollbar-face-color: #1584d6; /*图5,立体滚动条的颜色*/
scrollbar-3dlight-color: #06C; /*图1,立体滚动条亮边的颜色*/
scrollbar-highlight-color: #D5E8FA; /*图2,滚动条空白部分的颜色*/
scrollbar-shadow-color: #036; /*图3,立体滚动条阴影的颜色*/
scrollbar-darkshadow-color: #1584d6; /*图4,立体滚动条强阴影的颜色*/
scrollbar-track-color: #69b0dd; /*图7,立体滚动条背景颜色*/ 
scrollbar-base-color:#1b6486; /*滚动条的基本颜色*/
Cursor:url(mouse.cur); /*自定义个性鼠标*/
}

ul,ol,img,li{list-style-type:none; border:0px;  }
A:Link {color: #5a5a5a; text-decoration: none;}A:Visited {	text-decoration: none;color: #5a5a5a; }
A:Hover {text-decoration:none; color:#ff7002;}A:Active {text-decoration: none;color: #5a5a5a;}
A:Active, A:Visited{outline:none;blr:expression(this.onFocus=this.blur());}

#Connet{ width:1000px; height:auto; margin:auto; text-align:center;  overflow:hidden; }
#Body{ height:auto; overflow:hidden; margin-top:12px; margin-bottom:10px; }
#Top{ height:162px; overflow:hidden;  background: url(../Image/TopBg.gif) left top repeat-x;  }
#Left{ width:222px; height:480px; border:1px solid #95c2e6; float:left; overflow-y:auto; overflow-x:hidden; }
#Right{width:766px; height:480px; border:1px solid #95c2e6; float:right;  overflow:hidden;}
.RightBox{ padding:10px;}
.Title{ height:29px; background:url(../Image/Title.gif); overflow:hidden; padding-left:14px;}
.Title ul li{line-height:27px;color:#005197; font-weight:bold;  }
.Icon1{ background:url(../Image/Icon1.gif) left top no-repeat; padding-left:24px;}
.Icon2{ background:url(../Image/Icon2.gif) left top no-repeat; padding-left:24px;}

.MenuNormal { padding-left:20px; height:26px;line-height:24px;  color:#7e9ec9; background:url(../Image/TitleNormal.gif); overflow:hidden;}
.MenuNormal A:Link {color: #7e9ec9; text-decoration: none;}.MenuNormal A:Visited {	text-decoration: none;color: #7e9ec9; }
.MenuNormal A:Hover {text-decoration:none; color:#005197;}.MenuNormal A:Active {text-decoration: none;color: #7e9ec9;}

.Block{ display:block;}
.None{ display:none;}
/*公用 End*/

/*Top*/
.TopBox{ height:122px; overflow: hidden; }
.TopLogo{ width:239px; height:65px; float:left; background:url(../Image/Logo.png); margin:30px 0px 0px 50px; }
